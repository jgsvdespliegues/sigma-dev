{
  "version": 2,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|html))",
      "dest": "/$1"
    },
    {
      "src": "/success.html",
      "dest": "/success.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}